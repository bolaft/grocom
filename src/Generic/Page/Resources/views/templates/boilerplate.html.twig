<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        {% block head %}

            {% block head_meta %}
                <meta charset="utf-8">
                <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
                <meta name="description" content="">
                <meta name="viewport" content="width=device-width">
            {% endblock head_meta %}

            <title>
                {% block head_title %}
                    {% trans %}site.head.title{% endtrans %} - 
                {% endblock %}
            </title>

            {% block head_css %}
                {% stylesheets
                    '@GenericPage/Resources/public/css/bootstrap.min.css'
                    '@GenericPage/Resources/public/css/bootstrap-responsive.min.css'
                    '@GenericPage/Resources/public/css/html5boilerplate.css' 
                %}
                    <link rel="stylesheet"  href="{{ asset_url }}" >
                {% endstylesheets %}
            {% endblock %}

            {% block head_style %}
                <style>
                    body {
                        padding-top: 60px;
                        padding-bottom: 40px;
                    }
                </style>
            {% endblock %}

            {% block head_js %}
                {% javascripts '@GenericPage/Resources/public/js/modernizr-2.6.1-respond-1.1.0.min.js' %}
                    <script src="{{ asset_url }}"></script>
                {% endjavascripts %}
            {% endblock %}

        {% endblock head %}
    </head>
    <body>
        {% block body %}

            {% block body_chromeframe %}
                <!--[if lt IE 7]>
                    <p class=chromeframe>
                        {% trans %}boilerplate.browser.ancient{% endtrans %}
                    </p>
                <![endif]-->
            {% endblock body_chromeframe %}

            {% block body_header %}

            {% endblock body_header %}

            {% block body_main %}
                
            {% endblock body_main %}

            {% block body_footer %}

            {% endblock body_footer %}

            {% block body_js %}
                <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
                <script>window.jQuery || document.write('<script src="js/jquery-1.8.1.min.js"><\/script>')</script>
            
                {% if analytics_id is defined %}
                    <script>
                    var _gaq=[['_setAccount','{{ analytics_id }}'],['_trackPageview']];
                    (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
                    g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
                    s.parentNode.insertBefore(g,s)}(document,'script'));
                    </script>
                {% endif %}

                {% javascripts 
                    '@GenericPage/Resources/public/js/bootstrap.min.js'
                    '@GenericPage/Resources/public/js/html5boilerplate.js'
                    '@GenericPage/Resources/public/js/html5boilerplate-plugins.js'
                %}
                    <script src="{{ asset_url }}"></script>
                {% endjavascripts %}

            {% endblock body_js %}
            
        {% endblock body %}
    </body>
</html>
