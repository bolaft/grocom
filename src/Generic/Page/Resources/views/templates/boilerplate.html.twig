<!DOCTYPE html>
    <!--[if lt IE 7]>      
        <html class="no-js lt-ie9 lt-ie8 lt-ie7"> 
    <![endif]-->
    <!--[if IE 7]>         
        <html class="no-js lt-ie9 lt-ie8"> 
    <![endif]-->
    <!--[if IE 8]>         
        <html class="no-js lt-ie9"> 
    <![endif]-->
    <!--[if gt IE 8]><!--> 
        <html class="no-js"> 
    <!--<![endif]-->
    <head>
        {% block head %}

            {% block head_meta %}
                <meta charset="utf-8">
                <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
                <meta name="description" content="">
                <meta name="viewport" content="width=device-width">
            {% endblock %}

            <title>
                {% block head_title %}
                    {% trans %}head.title.prefix{% endtrans %}
                {% endblock %}
            </title>

            {% block head_css %}
                {% stylesheets '@GenericPage/Resources/public/css/*'  filter='yui_css,cssrewrite' %}
                    <link rel="stylesheet"  href="{{ asset_url }}" >
                {% endstylesheets %}
            {% endblock %}

            <style>
                {% block head_style %}
                    {# Extra CSS style goes there #}
                {% endblock %}
            </style>

            {% block head_js %}
                {% javascripts '@GenericPage/Resources/public/js/modernizr-2.6.1-respond-1.1.0.min.js'  filter='yui_js' %}
                    <script src="{{ asset_url }}"></script>
                {% endjavascripts %}
            {% endblock %}

        {% endblock %}
    </head>
    <body>
        <!--[if lt IE 7]>
            <p class=chromeframe>
                {% trans %}boilerplate.browser.ancient{% endtrans %}
            </p>
        <![endif]-->
        {% block body %}

            {% block body_content %}
                {# Actual page content goes there #}
            {% endblock %}

            {% block body_js %}
                <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
                <script>window.jQuery || document.write('<script src="js/jquery-1.8.1.min.js"><\/script>')</script>
            
                {% if analytics_id is defined %}
                    <script>
                    var _gaq=[['_setAccount','{{ analytics_id }}'],['_trackPageview']];
                    (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
                    g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
                    s.parentNode.insertBefore(g,s)}(document,'script'));
                    </script>
                {% endif %}

                {% javascripts 
                    '@GenericPage/Resources/public/js/bootstrap.min.js'
                    '@GenericPage/Resources/public/js/boilerplate.js'
                 filter='yui_js' %}
                    <script src="{{ asset_url }}"></script>
                {% endjavascripts %}

            {% endblock %}
            
        {% endblock %}
    </body>
</html>